class Volunteer():
    def __init__(self, name, city, status):
        self.name = name
        self.city = city
        self.status = status
    def get_volunteers(self):
        return f"Name:{self.name},"\
               f"City:{self.city},"\
               f"Status:{self.status}"

vol = Volunteer("Ivan Ivanov", "Moscow", "volunteer")
print(vol.get_volunteers())
